{
    "name": "g11n-service",
    "version": "0.0.0",
    "description": "g11n asset linker service for bridged systems",
    "author": "bridged.xyz authors",
    "private": false,
    "license": "MIT",
    "main": "lambda",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "sls offline start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@bridged.xyz/client-sdk": "0.0.1-14",
        "@nestjs/common": "^7.0.0",
        "@nestjs/core": "^7.0.0",
        "@nestjs/platform-express": "^7.0.0",
        "aws-lambda": "^1.0.6",
        "aws-sdk": "^2.783.0",
        "aws-serverless-express": "^3.3.8",
        "class-transformer": "^0.3.1",
        "class-validator": "^0.12.2",
        "dynamoose": "^2.4.1",
        "nanoid": "^3.1.16",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^6.5.4"
    },
    "devDependencies": {
        "@babel/core": "^7.11.0",
        "@babel/plugin-proposal-decorators": "^7.12.1",
        "@babel/plugin-transform-runtime": "^7.12.10",
        "@babel/plugin-transform-typescript": "^7.12.1",
        "@babel/preset-env": "^7.11.0",
        "@babel/preset-typescript": "^7.10.4",
        "@hewmen/serverless-plugin-typescript": "^1.1.17",
        "@nestjs/cli": "^7.0.0",
        "@nestjs/schematics": "^7.0.0",
        "@nestjs/testing": "^7.0.0",
        "@types/aws-lambda": "^8.10.64",
        "@types/express": "^4.17.3",
        "@types/jest": "26.0.10",
        "@types/node": "^13.9.1",
        "@types/supertest": "^2.0.8",
        "@typescript-eslint/eslint-plugin": "3.9.1",
        "@typescript-eslint/parser": "3.9.1",
        "babel-loader": "^8.1.0",
        "babel-plugin-module-resolver": "^4.0.0",
        "babel-plugin-parameter-decorator": "^1.0.16",
        "cache-loader": "^4.1.0",
        "eslint": "7.7.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-import": "^2.20.1",
        "fork-ts-checker-webpack-plugin": "^5.0.12",
        "jest": "26.4.2",
        "plugin": "^0.3.3",
        "prettier": "^1.19.1",
        "serverless-bundle": "^4.1.0",
        "serverless-domain-manager": "^5.1.0",
        "serverless-dynamodb-local": "^0.2.39",
        "serverless-offline": "^6.8.0",
        "serverless-plugin-monorepo": "^0.10.0",
        "serverless-plugin-optimize": "^4.1.4-rc.1",
        "serverless-plugin-typescript": "^1.1.9",
        "serverless-webpack": "^5.3.4",
        "supertest": "^4.0.2",
        "ts-jest": "26.2.0",
        "ts-loader": "^8.0.11",
        "ts-node": "9.0.0",
        "tsconfig-paths": "^3.9.0",
        "tsconfig-paths-webpack-plugin": "^3.3.0",
        "typescript": "^4.1.2",
        "webpack": "^5.10.0",
        "webpack-cli": "^4.2.0",
        "webpack-node-externals": "^2.5.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
